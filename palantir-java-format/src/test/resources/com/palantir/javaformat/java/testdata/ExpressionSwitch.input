class ExpressionSwitch {
  boolean odd(int x) {
    return switch (x) {
      case 0  -> true;
      case 1  -> false;
      default -> { int y = x - 1; return odd(y); }
    };
  }

  int yield(int i) {
    return switch (i) {
      case 0: yield 0;
      default: yield n/i;
    };
  }

  int assignment(int j) {
    boolean val = switch (j) {
        case 0 -> true;
        case 1 -> false;
        default -> false;
    };
    return val;
  }
}