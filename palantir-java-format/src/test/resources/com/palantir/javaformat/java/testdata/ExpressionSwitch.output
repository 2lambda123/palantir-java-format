class ExpressionSwitch {
    boolean odd(int x) {
        return switch (x) {
            case 0 -> true;
            case 1 -> false;
            default -> {
                int y = x - 1;
                return odd(y);
            }
        };
    }

    int yield() {
        return switch (i) {
            case 0:
                yield 0;
            default:
                yield n / i;
        };
    }
}
